{
    "defaultSeverity": "error",
    "extends": [
       "tslint:recommended"
    ],
    
    "ignoreDefinitionFiles":true,
    "jsRules": {},
    "rules": {
        "no-console": false,
        "arrow-parens":false,
        "forin": false,
        "object-literal-shorthand": false,
        "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]

    },
    "rulesDirectory": []
}